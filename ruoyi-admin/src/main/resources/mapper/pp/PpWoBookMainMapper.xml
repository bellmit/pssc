    <sql id="selectPpWoBookMainVo">
        select id, wcode, ptime, puser, pcode, pname, qty, curr_pro, pstar_time, pend_time, fqty, expect_ftime, progress, m.remark, m.is_valid, m.create_by, m.create_time, m.update_by, m.update_time, attribute1, attribute2, attribute3 
        from pp_wo_book_main m left join sys_user su on m.create_by = su.login_name 

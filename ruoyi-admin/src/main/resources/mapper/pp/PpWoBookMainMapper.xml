    <select id="selectPpWoBookMainList" parameterType="PpWoBookMain" resultMap="PpWoBookMainResult">
        <include refid="selectPpWoBookMainVo"/>
        <where>  
            <if test="pendStarttime != null "> and pend_time >= #{pendStarttime}</if>
            <if test="pendEndtime != null "> and pend_time  &lt;=#{pendEndtime}</if>
            <if test="wcode != null  and wcode != ''"> and wcode = #{wcode}</if>
            <if test="ptime != null "> and ptime = #{ptime}</if>
            <if test="puser != null  and puser != ''"> and puser = #{puser}</if>
            <if test="pcode != null  and pcode != ''"> and pcode = #{pcode}</if>
            <if test="pname != null  and pname != ''"> and pname like concat('%', #{pname}, '%')</if>
            <if test="qty != null "> and qty = #{qty}</if>
            <if test="currPro != null  and currPro != ''"> and curr_pro = #{currPro}</if>
            <if test="pstarTime != null "> and pstar_time = #{pstarTime}</if>
            <if test="pendTime != null "> and pend_time = #{pendTime}</if>
            <if test="fqty != null "> and fqty = #{fqty}</if>
            <if test="expectFtime != null "> and expect_ftime = #{expectFtime}</if>
            <if test="progress != null "> and progress = #{progress}</if>
            and is_valid = '1'
            <!-- 数据范围过滤 -->
            ${params.dataScope}
        </where>
        order by id desc
    </select>
    
